	doi = {https://doi.org/10.1016/j.patcog.2012.04.031},
	issn = {0031-3203},
	journal = {Pattern Recognition},
	keywords = {Cluster analysis, EM algorithm, Gaussian mixture model, Robust EM, Initialization, Number of clusters},
	number = {11},
